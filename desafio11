      "source": [
        "def diagonalMaior(lista, tamanho =4):\n",
        "  \n",
        "  maior = 0\n",
        "  produto = 1\n",
        "\n",
        "  #diagonal para baixo esquerda -> direita\n",
        "  for i in range (len(lista)-tamanho):\n",
        "    for x in range(tamanho):\n",
        "      produto *= lista[i+x][i+x]\n",
        "    if produto > maior:\n",
        "        maior = produto\n",
        "    \n",
        "    produto = 1\n",
        "  #diagonal para cimadireta -> esquerda\n",
        "  for i in range (3,len(lista)):\n",
        "    \n",
        "    for c in range(len(lista)-tamanho+1):\n",
        "      for x in range(tamanho-1,-1,-1):\n",
        "        produto *= lista[i-x][c+x]\n",
        "      if produto > maior:\n",
        "        maior = produto      \n",
        "      produto = 1\n",
        "    \n",
        "  \n",
        "  return maior\n",
        "     "
      ],
      "execution_count": 189,
      "outputs": []
    },
