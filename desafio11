      "source": [
        "def colunaMaior(lista, tamanho=4):\n",
        "  \n",
        "  maior = 0\n",
        "  produto = 1\n",
        "  \n",
        "  for i in range (len(lista)-tamanho):\n",
        "    for c in range (len(lista[i])):\n",
        "      for x in range(tamanho):\n",
        "        produto *= lista[i+x][c]\n",
        "      if produto > maior:\n",
        "        maior = produto\n",
        "      \n",
        "      produto = 1\n",
        "  \n",
        "  return maior\n"
      ],
      "execution_count": 188,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "TIXbg1cBzqZm",
        "colab_type": "code",
        "colab": {}
