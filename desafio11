      "source": [
        "def maiorProduto(lista, tamanho= 4):\n",
        "  maior = 0\n",
        "\n",
        "  if linhaMaior(lista, tamanho) > colunaMaior(lista, tamanho):\n",
        "    maior = linhaMaior(lista, tamanho)\n",
        "  else:\n",
        "    maior = colunaMaior(lista, tamanho)\n",
        "  \n",
        "  if maior < diagonalMaior(lista,tamanho):\n",
        "    maior = diagonalMaior(lista,tamanho)\n",
        "  \n",
        "  return maior"
      ],
