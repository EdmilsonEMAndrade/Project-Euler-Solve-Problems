      "source": [
        "def linhaMaior(lista,tamanho=4):\n",
        "  maior = 0\n",
        "  produto = 1\n",
        "\n",
        "  for i in range (len(lista)):\n",
        "    for c in range (len(lista[i])-tamanho):\n",
        "      for x in range(tamanho):\n",
        "        produto *= lista[i][c+x]\n",
        "        \n",
        "      if produto > maior:\n",
        "        maior = produto\n",
        "      \n",
        "      produto = 1\n",
        "  \n",
        "  return maior"
      ],
      "execution_count": 187,
      "outputs": []
    },
    {
      "cell_type": "code",
